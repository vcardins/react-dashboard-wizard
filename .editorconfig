# Editor configuration, see http://editorconfig.org
root = true

[*]
charset = utf-8
indent_style = tab
indent_size = 4
insert_final_newline = true
trim_trailing_whitespace = true
single_quote = true
quote_type = single
quote_props = consistent
single_attribute_per_line = true

[*.md]
max_line_length = off
trim_trailing_whitespace = false
